<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<Pane fx:id="mainHolder" prefHeight="459.0" prefWidth="576.0"
	xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="LoginController">
	<children>
		<AnchorPane>
		</AnchorPane>
		<GridPane layoutX="188.0" layoutY="185.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
					prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<TextField text="Login" />
				<TextField text="Password" GridPane.rowIndex="1" />
				<TextField GridPane.columnIndex="1" />
				<TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
			</children>
		</GridPane>
		<TextField layoutX="88.0" layoutY="88.0" prefHeight="44.0"
			prefWidth="434.0" text="Welcome in Personal Bank Account Manager">
			<font>
				<Font size="20.0" />
			</font>
		</TextField>
		<Button layoutX="335.0" layoutY="252.0" mnemonicParsing="false"
			onAction="#signInButton" text="Sign in" />
		<Button layoutX="188.0" layoutY="252.0" mnemonicParsing="false"
			text="Sign up" />
	</children>
</Pane>
